ALGORITHM problem 1
VAR
    ps,i,j: INTEGER
    t1,t2:  : ARRAY_OF FLOAT;
BEGIN
    FOR i FROM 0 TO t1.length STEP 1  DO
        Read(t1[i])
    END_FOR
      FOR j FROM 0 TO t2.length STEP 1  DO
        Read(t2[j])
    END_FOR
    FOR i FROM 0 TO t1.length STEP 1  DO
        ps:=ps + (t1[i] * t2[i])
    END_FOR
    IF (ps = 0) THEN
    ELSE
        Write("not orthogonaux")
    END_IF
END
